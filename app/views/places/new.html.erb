
 <%= form_for @place do |f| %>
    <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="field">
      <%= f.label :location %>
      <%= f.text_field :location %>
    </div>
    <div class="field">
      <%= f.label :equipment %>
      <%= f.text_area :equipment %>
    </div>
    <div class="field">
      <%= f.label :photo %>
      <%= f.file_field :photo %>
    </div>
    <div class="actions"><%= f.submit %></div>
  <% end %>